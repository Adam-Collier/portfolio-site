{
  "extends": ["wesbos"],
  "settings": {
    "import/core-modules": ["styled-jsx", "styled-jsx/css"]
  },
  "rules": {
    "react/prop-types": 0,
    "react/jsx-props-no-spreading": 0,
    "react/display-name": 0,
    "prettier/prettier": [
      "error",
      {
        "trailingComma": "es5",
        "singleQuote": true
      }
    ]
  }
}
